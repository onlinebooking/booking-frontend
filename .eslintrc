{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "settings": {
    "ecmascript": 6,
    "jsx": true
  },
  "plugins": [
    "react"
  ],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "no-extra-semi": 0,
    "comma-dangle": 0,
    "react/prop-types": 0,
    "no-case-declarations": 0,
    "no-unused-vars": ["error", { "vars": "all", "args": "none" }]
  },
  "globals": {
    "Promise": false,
    "Symbol": false
  }
}
